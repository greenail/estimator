<form action=/configs/add_config method=post>
<table>
<tr>
<th>Name</th>
<th>Type</th>
<th>Minimum</th>
<th>Maximum</th>
<th>Days to run Model</th>
<th>Off days utilization</th>
</tr>
<tr>
<td><input type=text name=name></td>
<td>
<select name=type>
<%
ami_types = open('ami_types.yaml') { |f| YAML.load(f) }
for type in ami_types.keys
	%><option value=<%= type %>><%= type %></option> <%	
end
%></select>
</td>
<td><input type=text name=min_q value=0 ></td>
<td><input type=text name=max_q value=100 ></td>
<td><input type=text name=days value=7 ></td>
<td><input type=text name=weekend_usage value=0.2></td>
</table>
<input type=submit value='add'>
