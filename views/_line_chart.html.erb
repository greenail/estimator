<%

chart = ""
chart_header = "http://chart.apis.google.com/chart?"
chart_type = "cht=lc"
chart_size = "&chs=400x250"
chart_label = "&chl=Test|Chart"
# <fill type>,s,<color>|
chart_background = "&chf=bg,s,E7E7E7"
chart_color = "&chco=006600"
# <marker type>,<color>,<data set index>,<data point>,<size>,<priority>|
chart_marker = "&chm=N*f0*,000000,0,-1,11"
# <axis 1>,<axis n> x = bottom x, t = top x-axis, y = left y-axis, r = right y-axis
chart_axis = "&chxt=x"
# <axis index>:|<label 1>|<label n>|
chart_axis_label = "0:|Mon|Tue|Wed|Thur|Fri"
# chart titel + for space | for new line
chart_title = "&chtt=Daily+Downloads|for+this plugin"
# chts=FF0000,20 or <color>, <size>
chart_title_color_and_size = "&chts=000000,16"

chart_data = "&chd=s:Paz9"

chart = chart_header + chart_type + chart_size + chart_label + chart_background + chart_color + chart_marker + chart_axis + chart_axis_label + chart_title  + chart_title_color_and_size + chart_data

%>
<img src="<%=chart%>">


