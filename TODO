first = true
min = 0
max = 0
for key in daily_matrix.keys.sort
        iu = daily_matrix[key]
        noi = iu[:number_of_instances]
        min = noi if noi.to_i < min.to_i
        max = noi if noi.to_i > max.to_i
        if (first)
                %><%= noi %><%
                first = false
        else
                %>,<%= noi %><%
        end
end
